{
    "compilerOptions": {
      "outDir": "./dist/",
      "module": "esnext",
      "target": "es5",
      "jsx": "react-jsx",
      "allowJs": true,
      "moduleResolution": "node",
      "lib": ["es6", "dom"],
      "noImplicitAny": true,
      "noImplicitThis": true,
      "strictNullChecks": true,
      "allowSyntheticDefaultImports": true
    }
  }